public class Main
{
    
  
    
    
    
    public static void helper(){
        System.out.println("Entered ............... ");
        for(int i = -1 ; i>=-3000000;i--){
            if(4567>>>i!=(4567>>>(i+32)%32)){
                System.out.println("NO TRUE");
                return;
            }
        }
        System.out.println("TRUE");
    }
    
    
    
	public static void main(String[] args) {
	    
	   
	   helper(); // ALWAYS TRUE
	    
	}
}
